{
  "name": "your_app_name",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animate.css": "^3.7.0",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-animated-css": "^1.0.4",
